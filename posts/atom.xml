---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>{{ site.name }}</title>
  <link href="http://{{ site.host }}/life/atom.xml" rel="self"/>
  <link href="http://{{ site.host }}/life/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://{{ site.host }}/life</id>
  <author>
    <name>Rei</name>
    <email>chloerei@gmail.com</email>
  </author>

  {% for post in site.categories.life limit:10 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="http://{{ site.host }}{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://{{ site.host }}{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}

</feed>
